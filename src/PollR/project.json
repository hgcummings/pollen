{
    "webroot" : "wwwroot",
    "exclude": "wwwroot/**/*.*",
    "dependencies": {
        "Kestrel": "1.0.0-*",
        "Microsoft.AspNet.Server.IIS": "1.0.0-*",
        "Microsoft.AspNet.Mvc": "6.0.0-*",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-*",
        "Microsoft.AspNet.Diagnostics": "1.0.0-*",
        "Microsoft.AspNet.StaticFiles": "1.0.0-*",
        "Microsoft.AspNet.SignalR.Server": "3.0.0-*",
        "Microsoft.WindowsAzure.ConfigurationManager":  "2.0.3",
        "WindowsAzure.Storage": "4.3.0",
        "Base3264-UrlEncoder": "1.0.1"
    },
    "commands": {
        "WebListener": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5002;http://192.168.0.10:5002",
        "Kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5004;http://192.168.0.10:5004",
        "run": "run server.urls=http://localhost:5003"
    },
    "frameworks" : {
        "aspnet50" : { },
        "aspnetcore50" : { }
    }
}
